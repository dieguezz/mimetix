{
  "name": "mimetix",
  "version": "0.0.1",
  "description": "Angular 1.5.8 ES6 component style",
  "main": "app/index.js",
  "scripts": {
    "start": "npm run dev:local",
    "clean": "rm -rf dist coverage",
    "lint": "eslint src",
    "dist": "babel-node ./webpack/dist-server",
    "dev": "babel-node ./webpack/dev-server",
    "dev:local": "npm run dev -- --base_tag=/",
    "dev:dev": "npm run dev -- --api=dev --app_env=dev",
    "compile": "webpack",
    "prebuild": "npm run clean && npm run lint",
    "build": "NODE_ENV=production npm run compile --",
    "build:dev": "npm run build -- --api=dev --app_env=dev",
    "build:qa": "npm run build -- --api=qa --app_env=qa",
    "build:uat": "npm run build -- --api=uat --app_env=uat",
    "build:prod": "npm run build -- --api=prod --app_env=prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dieguezz/angular-components.git"
  },
  "keywords": [
    "Angular",
    "ES6",
    "Webpack"
  ],
  "author": "dieguezz@gmail.com",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/dieguezz/angular-components/issues"
  },
  "homepage": "https://github.com/dieguezz/angular-components#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "babel-runtime": "^6.3.19",
    "browser-sync": "^2.11.1",
    "browser-sync-webpack-plugin": "^1.0.1",
    "colors": "^1.1.2",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.15.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.7.1",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "lodash": "^4.0.1",
    "ng-annotate-loader": "^0.1.0",
    "ngtemplate-loader": "^1.3.1",
    "node-libs-browser": "^0.5.3",
    "null-loader": "^0.1.1",
    "nyan-progress-webpack-plugin": "^1.1.4",
    "postcss-loader": "^0.8.0",
    "pushstate-server": "^1.8.1",
    "style-loader": "^0.13.0",
    "stylus": "^0.53.0",
    "stylus-loader": "^1.5.0",
    "underscore-template-loader": "^0.7.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1",
    "webpack-notifier": "^1.2.1",
    "webpack-stats-plugin": "^0.1.1",
    "yargs": "^3.32.0"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-aria": "^1.5.8",
    "angular-cookies": "^1.5.8",
    "angular-file-saver": "^1.1.2",
    "angular-material": "^1.1.1",
    "angular-messages": "^1.5.8",
    "angular-resource": "^1.5.8",
    "angular-slick-carousel": "^3.1.7",
    "angular-ui-router": "^0.3.1",
