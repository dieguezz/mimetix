  <base href="<%= htmlWebpackPlugin.options.templateOptions.baseTag %>">
</head>

<body>
  <h1 class="ng-hide">Mimetix: Dise√±o Web y Marketing Digital</h1>
  <angular-app ng-cloak></angular-app>
